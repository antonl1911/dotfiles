alias g="git"
alias emerge="sudo ionice -c 3 emerge"
alias temerge="sudo /usr/local/bin/temerge"
alias mount="sudo mount"
alias umount="sudo umount"
alias qemu="qemu -kernel-kqemu"
alias wine="LANG=\"ru_RU.UTF-8\" wine"
alias ll="ls -l"
alias la="ls -la"
alias lf="ls -f"
alias m="nice -n 19 make"
alias c="cd"
alias v="vim"
alias u="cd .."
alias less="$PAGER"
alias cs="cscope -d"
alias cn="cscope -b -q -R"
